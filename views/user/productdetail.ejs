                        </div>
                        <div id="img-container zoom3" class="carousel-item">
                            <img class="w-100 h-100" src="/images/<%= pd.images[2]  %>" alt="Image">
                        </div>
                    </div>
                    <a class="carousel-control-prev bg-black" href="#product-carousel" data-slide="prev">
                        <i class="fa fa-2x fa-angle-left text-dark"></i>
                    </a>
                    <a class="carousel-control-next bg-black " href="#product-carousel" data-slide="next">
                        <i class="fa fa-2x fa-angle-right text-dark"></i>
                    </a>
                </div>
                <div id="yourcar" style="position: relative;top:-500px;">

                </div>
            </div>

